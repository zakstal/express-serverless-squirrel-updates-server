{
  "name": "pinger-update-server",
  "version": "1.0.0",
  "description": "A boilerplate for using expressjs with serverless framework",
  "main": "lambda.js",
  "scripts": {
    "transpile": "babel --presets es2015 --plugins transform-react-jsx src --out-dir output",
    "start": "node_modules/.bin/webpack --config webpack.config.js && gulp && npm run transpile && export NODE_ENV='development' && node ./bin/www",
    "generate-proxy": "claudia generate-serverless-express-proxy --express-module app",
    "deploy": "gulp && npm run transpile && sls deploy"
  },
  "dependencies": {
    "aws-serverless-express": "^1.2.0",
    "babel-cli": "6.7.7",
    "babel-plugin-array-includes": "2.0.3",
    "babel-plugin-transform-es2015-classes": "6.7.7",
    "babel-plugin-transform-runtime": "6.7.5",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-runtime": "6.6.1",
    "body-parser": "~1.15.2",
    "compression": "^1.6.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "dotenv": "^2.0.0",
    "ejs": "~2.5.2",
    "es6-error": "3.0.0",
    "express": "~4.14.0",
    "github4": "0.6.0",
    "morgan": "~1.7.0",
    "newrelic": "1.27.2",
    "numeral": "1.5.3",
    "raven": "0.11.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "request": "2.72.0",
    "semver": "5.1.0",
    "serve-favicon": "~2.3.0",
    "superagent": "^3.5.2",
    "webpack": "^2.5.1"
  },
  "devDependencies": {
    "babel-eslint": "6.0.4",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.6.0",
    "eslint": "2.11.1",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "nodemon": "1.9.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SharathHuddar/express-serverless-boilerplate.git"
  },
  "keywords": [
    "express",
    "serverless",
    "lambda",
    "AWS",
    "API",
    "gateway"
  ],
  "author": "Sharath Huddar",
  "license": "ISC"
}
